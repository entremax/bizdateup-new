{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/theme/*": [
        "./src/theme/*"
      ],
      "@/lib/*": [
        "./src/lib/*",
        "./src/lib/utils.ts"
      ],
      "@/ui/*": [
        "./src/components/ui/*"
      ],
      "@/store/*": [
        "./src/store/*"
      ],
      "@/reducers/*": [
        "./src/store/features/reducers/*"
      ],
      "@/services/*": [
        "./src/store/features/services/*"
      ],
      "@/icon": [
        "./src/components/ui/icon"
      ],
      "@/icons/*": [
        "./src/components/ui/icons/*"
      ],
      "@/data": [
        "./src/data/index.tsx"
      ],
      "@/endpoint": [
        "./src/lib/endpoint/apiEndpoint.ts"
      ],
      "@/components/*": [
        "./src/app/(auth)/_components/*",
        "./src/app/dashboard/_components/*",
        "./src/app/invest/_component/*",
        "./src/components/mobileNavbar/*",
        "./src/components/navbar/*",
        "./src/components/*"
      ],
      "@/form-component/*": [
        "./src/app/profile/(profiles)/investor/_components/form_components/*"
      ],
      "@/profile-components/*": [
        "./src/app/profile/(profiles)/investor/_components/*",
        "./src/app/profile/(profiles)/investor/bank/components/*"
      ],
      "@/types/*": [
        "./src/types/*",
        "./src/app/invest/*",
        "./src/lib/endpoints/*"
      ],
      "@/typ": [
        "./src/types/profile",
        "./src/types"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
