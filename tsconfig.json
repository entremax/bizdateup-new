{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/theme/*": [
        "./src/theme/*"
      ],
      "@/action/*": [
        "./src/app/actions/*",
        "./src/app/referral/actions/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/ui/*": [
        "./src/components/ui/*"
      ],
      "@/store/*": [
        "./src/store/*"
      ],
      "@/reducers/*": [
        "./src/store/features/reducers/*"
      ],
      "@/services/*": [
        "./src/store/features/services/*"
      ],
      "@/icons/*": [
        "./src/components/icons/*"
      ],
      "@/data": [
        "./src/data/index.tsx"
      ],
      "@/components/common/*": [
        "./src/components/*",
        "./src/components/navbar/*"
      ],
      "@/components/form/*": [
        "./src/app/profile/investor/_components/form_components/*"
      ],
      "@/components/home/*": [
        "./src/app/_components/*"
      ],
      "@/components/auth/*": [
        "./src/app/(auth)/components/*"
      ],
      "@/components/dashboard/*": [
        "./src/app/dashboard/_components/*",
        "./src/app/dashboard/startup/components/*"
      ],
      "@/components/dashboard/startup/*": [
        "./src/app/dashboard/startup/_component/*"
      ],
      "@/components/learn/*": [
        "./src/app/learn/components/*",
        "./src/app/learn/faqs/components/*",
        "./src/app/learn/blogs/components/*",
        "./src/app/learn/about-us/components/*"
      ],
      "@/components/invest/*": [
        "./src/app/invest/components/*",
        "./src/app/invest/(invest_pages)/startup/_component/*"
      ],
      "@/components/startup/*": [
        "./src/app/startup/components/*"
      ],
      "@/components/payment/*": [
        "./src/app/payment/_components/*"
      ],
      "@/components/profile/startup/*": [
        "./src/app/profile/startup/_components/*",
        "./src/app/profile/startup/deal-terms/_components/*",
        "./src/app/profile/startup/upload-docs/_components/*",
        "./src/app/profile/startup/events/_components/*",
        "./src/app/profile/startup/mentor/_components/*",
        "./src/app/profile/startup/team/_components/*",
        "./src/app/profile/startup/faqs/_components/*",
        "./src/app/profile/startup/pitch/_components/*"
      ],
      "@/components/profile/*": [
        "./src/app/profile/investor/_components/*",
        "./src/app/profile/startup/_components/*",
        "./src/app/profile/investor/bank/components/*",
        "./src/app/profile/investor/other/components/*"
      ],
      "@/components/portfolio/*": [
        "./src/app/portfolio/components/*"
      ],
      "@/components/referral/*": [
        "./src/app/referral/components/*"
      ],
      "@/context/profile/*": [
        "./src/app/profile/investor/_components/context/*",
        "./src/app/profile/startup/_components/context/*"
      ],
      "@/context/*": [
        "./src/context/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
