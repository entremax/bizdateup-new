{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/theme/*": [
        "./src/theme/*"
      ],
      "@/action/*": [
        "./src/app/actions/*",
        "./src/app/referral/actions/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/ui/*": [
        "./src/components/ui/*"
      ],
      "@/store/*": [
        "./src/store/*"
      ],
      "@/reducers/*": [
        "./src/store/features/reducers/*"
      ],
      "@/services/*": [
        "./src/store/features/services/*"
      ],
      "@/icons/*": [
        "./src/components/icons/*"
      ],
      "@/data": [
        "./src/data/index.tsx"
      ],
      "@/components/common/*": [
        "./src/components/*",
        "./src/components/navbar/*"
      ],
      "@/components/form/*": [
        "./src/app/profile/(profiles)/investor/_components/form_components/*"
      ],
      "@/components/auth/*": [
        "./src/app/(auth)/components/*"
      ],
      "@/components/dashboard/*": [
        "./src/app/dashboard/_components/*",
        "./src/app/dashboard/startup/components/*"
      ],
      "@/components/invest/*": [
        "./src/app/invest/_component/*"
      ],
      "@/components/payment/*": [
        "./src/app/payment/_components/*"
      ],
      "@/components/profile/*": [
        "./src/app/profile/(profiles)/investor/_components/*",
        "./src/app/profile/(profiles)/investor/bank/components/*",
        "./src/app/profile/(profiles)/investor/other/components/*"
      ],
      "@/components/referral/*": [
        "./src/app/referral/components/*"
      ],
      "@/context/profile/*": [
        "./src/app/profile/(profiles)/investor/_components/context/*"
      ],
      "@/hooks/common/*": [
        "./src/hooks/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
